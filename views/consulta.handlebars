<div class="container">
    <div class="row">
        <div class="col">
            <nav class="navbar navbar-expand-lg bg-primary text-light">
                <div class="container-fluid" >
                    <a class="navbar-brand text-light" href="#">Sistema de Agendamentos</a>
                        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
                            <span class="navbar-toggler-icon"></span>
                        </button>
                        <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
                        <div class="navbar-nav">
                            <a class="nav-link active text-light" aria-current="page" href="/">Cadastrar</a>
                            <a class="nav-link text-light" href="/consulta">Consultar</a>
                        </div>
                    </div>
                </div>
            </nav>
        </div>
    </div>
    <div class="row">
        <div class="col">
            &nbsp;
        </div>
    </div>
    <div class="row">
        <div class="col bg-white">
            <p class="text-start fs-5">Consultar</p>
            <p class="text-start fs-6">Lista de dados cadastrados</p>
            <p>
            <div class="container text-center">
                <div class="row bg-primary">
                    <div class="col text-light">
                        Nome
                    </div>
                    <div class="col text-light">
                        Endereço
                    </div>
                    <div class="col text-light">
                        Bairro
                    </div>
                    <div class="col text-light">
                        Cep
                    </div>
                    <div class="col text-light">
                        Cidade
                    </div>
                    <div class="col text-light">
                        Estado
                    </div>
                    <div class="col text-light">
                        Telefone
                    </div>
                    <div class="col text-light">
                        Celular
                    </div>
                    <div class="col text-light">
                        Ações
                    </div>
                </div>
                <div class="row">
                    <div class="col text-light">
                        &nbsp;
                    </div>
                    <div class="col text-light">
                        &nbsp;
                    </div>
                    <div class="col text-light">
                        &nbsp;
                    </div>
                    <div class="col text-light">
                        &nbsp;
                    </div>
                </div>
                {{#each posts}}
                    {{#dataValues}}
                        <div class="row">
                            <div class="col">
                                {{nome}}
                            </div>
                            <div class="col">
                                {{endereco}}
                            </div>
                            <div class="col">
                                {{bairro}}
                            </div>
                            <div class="col">
                                {{cep}}
                            </div>
                            <div class="col">
                                {{cidade}}
                            </div>
                            <div class="col">
                                {{estado}}
                            </div>
                            <div class="col">
                                {{telefone}}
                            </div>
                            <div class="col">
                                {{celular}}
                            </div>
                            <div class="col">
                                <a class="btn btn-primary btn-sm" style="--bs-btn-padding-y: .25rem; --bs-btn-padding-x: .2rem; --bs-btn-font-size: .68rem;" href="/editar/{{id}}">Editar</a>
                                <a class="btn btn-primary btn-sm" style="--bs-btn-padding-y: .25rem; --bs-btn-padding-x: .2rem; --bs-btn-font-size: .68rem;" href="/excluir/{{id}}" >Excluir</a>
                            </div>
                        </div>
                    {{/dataValues}}
                {{/each}}
            </div>
        </div>
    </div> 
</div>